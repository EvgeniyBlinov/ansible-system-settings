[color]
	diff = auto
	status = auto
	branch = auto
	ui = auto
	interactive = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = cyan bold
	changed = red
	untracked = magenta bold
