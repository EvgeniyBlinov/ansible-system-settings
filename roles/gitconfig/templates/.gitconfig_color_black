; [include]
;			path = ~/.gitconfig_color_black
[color]
	ui = true
[color "branch"]
	current = yellow black
	local = yellow
	remote = magenta
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red reverse
	new = green reverse
	whitespace = white reverse
[color "status"]
	added = yellow
	changed = green
	untracked = cyan reverse
	branch = magenta
